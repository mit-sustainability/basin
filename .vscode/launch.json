    "version": "0.2.0",
    "configurations": [
        {
            "name": "dagster dev",
            "type": "python",
            "request": "launch",
            "module": "dagster",
            "args": [
                "dev",
                "-w",
                "workspace.yaml"
            ],
            "env": {
                "DAGSTER_HOME": "${workspaceFolder}/orchestrator/.dagster",
                "DAGSTER_LOCAL_MODE": "true"
            },
            "cwd": "${workspaceFolder}/orchestrator"
        }
    ]
}
